{
  "name": "exp-wisvids",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "testvar": "echo $FIREBASE_TOKEN",
    "addUserClaims": "export GOOGLE_APPLICATION_CREDENTIALS=/var/www/svelte/adminkey.json; node scripts/addUserClaims.js",
    "deploy": "bash scripts/deploy.sh",
    "emulate": "firebase --token \"$FIREBASE_TOKEN\" emulators:start  --import=./.devdata --export-on-exit",
    "test-setup": "export GOOGLE_APPLICATION_CREDENTIALS=/var/www/svelte/adminkey.json; firebase emulators:start --config=\"./firebase.test.json\""
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.2.5",
    "cssnano": "^3.10.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "firebase-admin": "^9.8.0",
    "postcss": "^8.3.4",
    "postcss-load-config": "^3.0.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.3.0",
    "svelte": "^3.29.0",
    "svelte-adapter-firebase": "^0.8.0",
    "svelte-preprocess": "^4.7.1",
    "tailwindcss": "^2.0.4",
    "vite": "^2.1.0"
  },
  "type": "module",
  "dependencies": {
    "@algolia/autocomplete-js": "^1.0.1",
    "@algolia/autocomplete-theme-classic": "^1.0.1",
    "@fontsource/fira-mono": "^4.2.2",
    "@lukeed/uuid": "^2.0.0",
    "@tailwindcss/forms": "^0.3.2",
    "@vimeo/player": "^2.15.0",
    "algoliasearch": "^4.9.1",
    "cookie": "^0.4.1",
    "firebase": "^9.0.0-beta.1",
    "firebase-functions": "^3.14.1",
    "katex": "^0.13.11",
    "svelte-class-transition": "^1.0.4"
  }
}
