{
  "name": "exp-wisvids",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "testvar": "echo $FIREBASE_TOKEN",
    "addUserClaims": "export GOOGLE_APPLICATION_CREDENTIALS=/var/www/svelte/adminkey.json; node scripts/addUserClaims.js",
    "deploy": "bash scripts/deploy.sh",
    "emulate": "node scripts/generateConfig.js; firebase emulators:start  --import=./data/.devdata --export-on-exit --only auth,functions,firestore,pubsub",
    "emulateCypress": "node scripts/generateConfig.js; firebase emulators:start  --import=./data/.cypressdata --export-on-exit --only auth,functions,firestore,pubsub",
    "test-setup": "export GOOGLE_APPLICATION_CREDENTIALS=/var/www/svelte/adminkey.json; firebase emulators:start --config=\"./firebase.test.json\""
  },
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.180",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "firebase-admin": "^9.12.0",
    "firebase-tools": "^9.22.0",
    "json-ptr": "^3.0.0",
    "postcss": "^8.3.11",
    "postcss-load-config": "^3.0.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.3.0",
    "svelte": "^3.44.1",
    "svelte-adapter-firebase": "^0.13.1",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.19",
    "vite": "^2.6.14"
  },
  "type": "module",
  "dependencies": {
    "@algolia/autocomplete-js": "^1.5.0",
    "@algolia/autocomplete-theme-classic": "^1.5.0",
    "@fontsource/fira-mono": "^4.2.2",
    "@lukeed/uuid": "^2.0.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@vimeo/player": "^2.16.2",
    "algoliasearch": "^4.11.0",
    "cookie": "^0.4.1",
    "diff-match-patch": "^1.0.5",
    "firebase": "^8.0.0",
    "javascript-time-ago": "^2.3.10",
    "katex": "^0.13.24",
    "leaflet": "^1.7.1",
    "svelte-class-transition": "^1.0.4"
  }
}
